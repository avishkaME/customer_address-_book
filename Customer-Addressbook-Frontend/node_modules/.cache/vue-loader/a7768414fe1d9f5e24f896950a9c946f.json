lid)\"\r\n    />\r\n    <span v-if=\"v$.telephoneNumber.$error && submitted\">\r\n      <span v-for=\"(error, index) of v$.telephoneNumber.$errors\" :key=\"index\">\r\n        <div>\r\n          <small class=\"p-error\">{{ error.$message }}</small>\r\n        </div>\r\n      </span>\r\n    </span>\r\n    <small\r\n      v-else-if=\"\r\n        (v$.telephoneNumber.$invalid && submitted) ||\r\n        v$.telephoneNumber.$pending.$response\r\n      \"\r\n      class=\"p-error\"\r\n      >{{\r\n        v$.telephoneNumber.required.$message.replace(\"Value\", \"Telephone\")\r\n      }}</small\r\n    >\r\n  </div>\r\n</template>\r\n<script>\r\nimport {\r\n  required,\r\n  minLength,\r\n  maxLength,\r\n  numeric,\r\n  helpers,\r\n} from \"@vuelidate/validators\";\r\nimport { useVuelidate } from \"@vuelidate/core\";\r\n\r\nexport default {\r\n  props: {\r\n    index: {\r\n      type: Number,\r\n      required: true,\r\n    },\r\n  },\r\n  setup: () => ({ v$: useVuelidate() }),\r\n  data() {\r\n    return {\r\n      telephoneNumber: \"\",\r\n      submitted: false,\r\n    };\r\n  },\r\n  validations() {\r\n    return {\r\n      telephoneNumber: {\r\n        required,\r\n        numeric: helpers.withMessage(\r\n          \"Contact number should have only numbers\",\r\n          numeric\r\n        ),\r\n        minLength: minLength(10),\r\n        maxLength: maxLength(10),\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    handleSubmit(isFormValid) {\r\n      this.submitted = true;\r\n\r\n      if (!isFormValid) {\r\n        return;\r\n      } else {\r\n        if (this.telephoneNumber.length == 10 && isFormValid) {\r\n          //   document.getElementById(\"telephone\").disabled = true;\r\n          const objectToSend = {\r\n            arrayIndex: this.index,\r\n            telephoneNumber: this.telephoneNumber,\r\n          };\r\n          this.$emit(\"input\", objectToSend);\r\n        }\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n"]}]}