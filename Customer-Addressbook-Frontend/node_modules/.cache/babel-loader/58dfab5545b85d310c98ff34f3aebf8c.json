aP,IAAA,aAbO,2BAaS;AAAA;;AACd,WAAK,SAAL,GAAiB,KAAK,SAAL,CAAe,MAAf,CAAsB,UAAC,GAAD;AAAA,eAAS,GAAG,CAAC,EAAJ,KAAW,MAAI,CAAC,OAAL,CAAa,EAAjC;AAAA,OAAtB,CAAjB;AACA,WAAK,mBAAL,GAA2B,KAA3B;AACA,WAAK,OAAL,GAAe,EAAf;AACA,WAAK,MAAL,CAAY,GAAZ,CAAgB;AACd,QAAA,QAAQ,EAAE,SADI;AAEd,QAAA,OAAO,EAAE,YAFK;AAGd,QAAA,MAAM,EAAE,iBAHM;AAId,QAAA,IAAI,EAAE;AAJQ,OAAhB;AAMD,KAvBM;AAwBP,IAAA,qBAxBO,mCAwBiB;AACtB,WAAK,oBAAL,GAA4B,IAA5B;AACD,KA1BM;AA2BP,IAAA,sBA3BO,oCA2BkB;AAAA;;AACvB,WAAK,QAAL,GAAgB,KAAK,QAAL,CAAc,MAAd,CACd,UAAC,GAAD;AAAA,eAAS,CAAC,MAAI,CAAC,gBAAL,CAAsB,QAAtB,CAA+B,GAA/B,CAAV;AAAA,OADc,CAAhB;AAGA,WAAK,oBAAL,GAA4B,KAA5B;AACA,WAAK,gBAAL,GAAwB,IAAxB;AACA,WAAK,MAAL,CAAY,GAAZ,CAAgB;AACd,QAAA,QAAQ,EAAE,SADI;AAEd,QAAA,OAAO,EAAE,YAFK;AAGd,QAAA,MAAM,EAAE,kBAHM;AAId,QAAA,IAAI,EAAE;AAJQ,OAAhB;AAMD,KAvCM;AAwCP,IAAA,WAxCO,yBAwCO;AACZ,WAAK,OAAL,GAAe;AACb,QAAA,MAAM,EAAE;AAAE,UAAA,KAAK,EAAE,IAAT;AAAe,UAAA,SAAS,EAAE,eAAe,CAAC;AAA1C;AADK,OAAf;AAGD;AA5CM;AArBI,CAAf","sourcesContent":["<template>\n  <div class=\"p-m-5\">\n    <div class=\"card\">\n      <!-- <Toolbar class=\"p-mb-4\">\n        <template #left>\n          <h2>Customer List</h2>\n        </template>\n      </Toolbar> -->\n\n      <DataTable\n        ref=\"dt\"\n        :value=\"customers\"\n        v-model:selection=\"selectedProducts\"\n        dataKey=\"id\"\n        :paginator=\"true\"\n        :rows=\"10\"\n        :filters=\"filters\"\n        paginatorTemplate=\"FirstPageLink PrevPageLink PageLinks NextPageLink LastPageLink CurrentPageReport RowsPerPageDropdown\"\n        :rowsPerPageOptions=\"[5, 10, 25]\"\n        currentPageReportTemplate=\"Showing {first} to {last} of {totalRecords} products\"\n        responsiveLayout=\"scroll\"\n      >\n        <template #header>\n          <div\n            class=\"\n              table-header\n              p-d-flex p-flex-column p-flex-md-row p-jc-md-between\n            \"\n          >\n            <h5 class=\"p-mb-2 p-m-md-0 p-as-md-center\">Customer List</h5>\n            <span class=\"p-input-icon-left\">\n              <i class=\"pi pi-search\" />\n              <InputText\n                v-model=\"filters['global'].value\"\n                placeholder=\"Search...\"\n              />\n            </span>\n          </div>\n        </template>\n\n        <Column\n          field=\"name\"\n          header=\"Name\"\n          :sortable=\"true\"\n          style=\"min-width: 12rem\"\n        ></Column>\n        <Column\n          field=\"nic\"\n          header=\"NIC\"\n          :sortable=\"true\"\n          style=\"min-width: 16rem\"\n        ></Column>\n        <Column\n          field=\"address\"\n          header=\"Address\"\n          :sortable=\"true\"\n          style=\"min-width: 8rem\"\n        >\n          <template #body=\"slotProps\">\n            <span>{{slotProps.data.address.address}}</span>\n          </template>\n        </Column>\n        <Column\n          field=\"telephones\"\n          header=\"Telephones\"\n          :sortable=\"true\"\n          style=\"min-width: 10rem\"\n          >\n            <template #body=\"slotProps\">\n              <span v-for=\"\">{{slotProps.data.telephones}}</span>\n            </template>\n          </Column\n        >\n        <Column :exportable=\"false\">\n          <template #body=\"slotProps\">\n            <!-- <Button\n              icon=\"pi pi-pencil\"\n              class=\"p-button-rounded p-button-success p-mr-2\"\n              @click=\"editProduct(slotProps.data)\"\n            /> -->\n            <Button\n              icon=\"pi pi-trash\"\n              class=\"p-button-rounded p-button-warning\"\n              @click=\"confirmDeleteProduct(slotProps.data)\"\n            />\n          </template>\n        </Column>\n      </DataTable>\n    </div>\n\n    <Dialog\n      v-model:visible=\"deleteProductDialog\"\n      :style=\"{ width: '450px' }\"\n      header=\"Confirm\"\n      :modal=\"true\"\n    >\n      <div class=\"confirmation-content\">\n        <i class=\"pi pi-exclamation-triangle p-mr-3\" style=\"font-size: 2rem\" />\n        <span v-if=\"product\"\n          >Are you sure you want to delete <b>{{ product.name }}</b\n          >?</span\n        >\n      </div>\n      <template #footer>\n        <Button\n          label=\"No\"\n          icon=\"pi pi-times\"\n          class=\"p-button-text\"\n          @click=\"deleteProductDialog = false\"\n        />\n        <Button\n          label=\"Yes\"\n          icon=\"pi pi-check\"\n          class=\"p-button-text\"\n          @click=\"deleteProduct\"\n        />\n      </template>\n    </Dialog>\n  </div>\n</template>\n\n<script>\nimport { FilterMatchMode } from \"primevue/api\";\nimport Customer from \"../../src/apis/Customers\";\n\nexport default {\n  data() {\n    return {\n      customers: [],\n      productDialog: false,\n      deleteProductDialog: false,\n      product: {},\n      selectedProducts: null,\n      filters: {},\n      submitted: false,\n    };\n  },\n  created() {\n    this.initFilters();\n  },\n  mounted() {\n    Customer.getCustomers().then((resp) => {\n      console.log(resp.data.data);\n      this.customers = resp.data.data;\n    })\n  },\n  methods: {\n    hideDialog() {\n      this.productDialog = false;\n      this.submitted = false;\n    },\n    // editProduct(product) {\n    //   this.product = { ...product };\n    //   this.productDialog = true;\n    // },\n    confirmDeleteProduct(product) {\n      this.product = product;\n      this.deleteProductDialog = true;\n    },\n    deleteProduct() {\n      this.customers = this.customers.filter((val) => val.id !== this.product.id);\n      this.deleteProductDialog = false;\n      this.product = {};\n      this.$toast.add({\n        severity: \"success\",\n        summary: \"Successful\",\n        detail: \"Product Deleted\",\n        life: 3000,\n      });\n    },\n    confirmDeleteSelected() {\n      this.deleteProductsDialog = true;\n    },\n    deleteSelectedProducts() {\n      this.products = this.products.filter(\n        (val) => !this.selectedProducts.includes(val)\n      );\n      this.deleteProductsDialog = false;\n      this.selectedProducts = null;\n      this.$toast.add({\n        severity: \"success\",\n        summary: \"Successful\",\n        detail: \"Products Deleted\",\n        life: 3000,\n      });\n    },\n    initFilters() {\n      this.filters = {\n        global: { value: null, matchMode: FilterMatchMode.CONTAINS },\n      };\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.table-header {\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n\n  @media screen and (max-width: 960px) {\n    align-items: start;\n  }\n}\n\n.product-image {\n  width: 50px;\n  box-shadow: 0 3px 6px rgba(0, 0, 0, 0.16), 0 3px 6px rgba(0, 0, 0, 0.23);\n}\n\n.p-dialog .product-image {\n  width: 50px;\n  margin: 0 auto 2rem auto;\n  display: block;\n}\n\n.confirmation-content {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n@media screen and (max-width: 960px) {\n  ::v-deep(.p-toolbar) {\n    flex-wrap: wrap;\n\n    .p-button {\n      margin-bottom: 0.25rem;\n    }\n  }\n}\n</style>"],"sourceRoot":""}]}